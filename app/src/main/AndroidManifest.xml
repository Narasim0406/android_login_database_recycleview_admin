<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.narasim.jm_solution_admin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/admin"
        android:label="@string/app_name"
        android:roundIcon="@drawable/admin"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="MainActivity" />

        <activity android:name=".MainActivity" android:screenOrientation="nosensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".ProfileActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".VerifyOtp"
            android:parentActivityName=".LoginActivity"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ResetOtp"
            android:parentActivityName=".LoginActivity"
            android:screenOrientation="nosensor"/>

        <activity
            android:name=".Report"
            android:parentActivityName=".ProfileActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".Feedback"
            android:parentActivityName=".ProfileActivity"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ReportProduct"
            android:parentActivityName=".Report"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ReportService"
            android:parentActivityName=".Report"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".FeedbackProduct"
            android:parentActivityName=".Feedback"
            android:screenOrientation="nosensor" />
        <activity
            android:name=".FeedbackService"
            android:parentActivityName=".Feedback"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".FeedbackProductDetail"
            android:parentActivityName=".FeedbackProduct"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".FeedbackServiceDetail"
            android:parentActivityName=".FeedbackService"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ReportProductCompleted"
            android:parentActivityName=".ReportProduct"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ReportProductProgress"
            android:parentActivityName=".ReportProduct"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ReportProductCancelled"
            android:parentActivityName=".ReportProduct"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ReportServiceProgress"
            android:parentActivityName=".ReportService"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ReportServiceCompleted"
            android:parentActivityName=".ReportService"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ReportServiceCancelled"
            android:parentActivityName=".ReportService"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ReportServiceDetail"
            android:parentActivityName=".ReportService"
            android:screenOrientation="nosensor"/>
        <activity
            android:name=".ReportProductDetail"
            android:parentActivityName=".ReportProduct"
            android:screenOrientation="nosensor"></activity>
    </application>


</manifest>